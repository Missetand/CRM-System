<div class="row">
{{#each model as |architects|}}
    <div class="col-md-4">
        {{#customer-item item=architects}}
            {{#link-to 'architects.edit' architects.id class='btn btn-success btn-xs'}}Edit{{/link-to}}
            <button class="btn btn-danger btn-xs" {{action 'deleteArchitects' architects}}>Delete</button>
        {{/customer-item}}
    </div>
{{/each}}
</div>