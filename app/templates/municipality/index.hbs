<div class="row">
{{#each model as |municipality|}}
    <div class="col-md-4">
        {{#customer-item item=municipality}}
            {{#link-to 'municipality.edit' municipality.id class='btn btn-success btn-xs'}}Edit{{/link-to}}
            <button class="btn btn-danger btn-xs" {{action 'deleteMunicipality' municipality}}>Delete</button>
        {{/customer-item}}
    </div>
{{/each}}
</div>