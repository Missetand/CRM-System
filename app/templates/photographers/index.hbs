<div class="row">
{{#each model as |photographers|}}
    <div class="col-md-4">
        {{#customer-item item=photographers}}
            {{#link-to 'photographers.edit' photographers.id class='btn btn-success btn-xs'}}Edit{{/link-to}}
            <button class="btn btn-danger btn-xs" {{action 'deletePhotographers' photographers}}>Delete</button>
        {{/customer-item}}
    </div>
{{/each}}
</div>