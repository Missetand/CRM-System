<div class="row">
{{#each model as |media|}}
    <div class="col-md-4">
        {{#customer-item item=media}}
            {{#link-to 'media.edit' media.id class='btn btn-success btn-xs'}}Edit{{/link-to}}
            <button class="btn btn-danger btn-xs" {{action 'deleteMediaHouses' media}}>Delete</button>
        {{/customer-item}}
    </div>
{{/each}}
</div>