<div class="row">
{{#each model as |developers|}}
    <div class="col-md-4">
        {{#customer-item item=developers}}
            {{#link-to 'developers.edit' developers.id class='btn btn-success btn-xs'}}Edit{{/link-to}}
            <button class="btn btn-danger btn-xs" {{action 'deleteDevelopers' developers}}>Delete</button>
        {{/customer-item}}
    </div>
{{/each}}
</div>